{
  "name": "express-rest-api-2018",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "yarn flow:install:typed",
    "build": "babel src --out-dir dist/src --ignore spec.js && babel config --out-dir dist/config --copy-files",
    "build:batch": "babel batch --out-dir dist/batch --ignore spec.js && babel config --out-dir dist/config --copy-files",
    "start:dev": "NODE_ENV=development && yarn db:migrate && nodemon --exec babel-node src/index.js",
    "start:stg": "NODE_ENV=staging && yarn build && yarn db:migrate && babel-node dist/src/index.js",
    "start:prod": "NODE_ENV=production && yarn build && yarn db:migrate && babel-node dist/src/index.js",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:stg": "docker-compose -f docker-compose.yml -f docker-compose.stg.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "db": "sequelize --config config/config.js",
    "flow": "flow",
    "flow:install:typed": "flow-typed install",
    "test": "yarn test:lint && yarn test:unit",
    "test:unit": "yarn mocha 'src/**/*.spec.js'",
    "test:unit:controllers": "yarn mocha 'src/controllers/**/*.spec.js'",
    "test:unit:models": "yarn mocha 'src/*.spec.js'",
    "test:unit:middlewares": "yarn mocha 'src/middlewares/*.spec.js'",
    "test:unit:services": "yarn mocha 'src/services/*.spec.js'",
    "test:unit:target": "yarn mocha",
    "mocha": "NODE_ENV=test mocha --require mocha-babel --timeout 20000 --exit",
    "test:lint": "eslint ./src/"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.4.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.0",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "airbrake-js": "^1.1.0",
    "aws-sdk": "^2.205.0",
    "axios": "^0.18.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "iconv-lite": "^0.4.21",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "log4js": "^2.5.3",
    "moment": "^2.21.0",
    "moment-range": "^3.1.1",
    "moment-timezone": "^0.5.14",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.2",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^4.0.0"
  }
}
